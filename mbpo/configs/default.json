{
  "env_name": "",
  "env_buffer":{
    "max_buffer_size": 1000000
  },
  "model_buffer":{
    "max_buffer_size": 400000
  },
  "agent":{
    "gamma": 0.99,
    "target_smoothing_tau": 0.005,
    "alpha": 0.2,
    "q_network":{
      "hidden_dims": [256,256],
      "optimizer_class": "Adam",
      "learning_rate":0.0003,
      "act_fn": "relu",
      "out_act_fn": "identity"
    },
    "policy_network":{
      "hidden_dims": [256,256],
      "optimizer_class": "Adam",
      "deterministic": false,
      "learning_rate":0.0003,
      "act_fn": "relu",
      "out_act_fn": "identity",
      "re_parameterize": true
    },
    "entropy":{
      "automatic_tuning": true,
      "learning_rate": 0.0003,
      "optimizer_class": "Adam"
    }
  },

  "transition_model":{
    "model_batch_size": 256,
    "use_weight_decay": true,
    "optimizer_class": "Adam",
    "learning_rate":0.001,
    "holdout_ratio": 0.1,
    "inc_var_loss": true,
    "model":{
      "hidden_dims": [200, 200, 200, 200],
      "decay_weights": [0.000025, 0.00005, 0.000075, 0.000075, 0.0001],
      "act_fn": "swish",
      "out_act_fn": "identity",
      "num_elite": 5,
      "ensemble_size": 7
    }
  },
  "rollout_step_scheduler":{
    "initial_val": 1,
    "target_val": 15,
    "start_timestep": 20,
    "end_timestep": 100,
    "schedule_type": "linear"
  },
  "trainer":{
    "max_epoch": 125,
    "agent_batch_size": 256,
    "rollout_batch_size": 100000,
    "rollout_mini_batch_size": 10000,
    "model_retain_epochs": 1,
    "num_env_steps_per_epoch": 1000,
    "train_model_interval": 250,
    "train_agent_interval": 1,
    "max_trajectory_length":1000,
    "test_interval": 1,
    "num_test_trajectories": 5,
    "save_model_interval": 2000,
    "start_train_model_timestep": 5000,
    "start_train_agent_timestep": 1000,
    "save_video_demo_interval": 1,
    "log_interval": 1,
    "model_env_ratio": 0.95,
    "num_model_rollouts": 400,
    "num_agent_updates_per_env_step": 20,
    "max_agent_updates_per_env_step": 5,
    "max_model_update_epochs_to_improve": 5,
    "train_log_interval":50
  }
  
}
